{
    "name": "Obsidian Nextcloud Media AI Bot",
    "url": "https://obsidian-nextcloud.com",
    "hook_attributes": {
        "url": "https://YOUR_GATEWAY_URL/webhooks/github"
    },
    "redirect_url": "https://YOUR_GATEWAY_URL/webhooks/github/callback",
    "public": false,
    "default_permissions": {
        "issues": "write",
        "pull_requests": "write",
        "contents": "read",
        "metadata": "read"
    },
    "default_events": [
        "issues",
        "issue_comment",
        "pull_request",
        "push"
    ]
}